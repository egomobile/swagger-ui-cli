{
    "name": "swagger-ui-cli",
    "displayName": "Swagger UI CLI",
    "version": "0.15.0",
    "description": "A standalone CLI application, serving Swagger UIs via a HTTP server.",
    "exports": "./dist/index.js",
    "type": "module",
    "private": false,
    "bin": {
        "swagger-ui": "./bin/swagger-ui.mjs"
    },
    "files": [
        "bin",
        "dist"
    ],
    "keywords": [
        "swagger",
        "ui",
        "swagger ui",
        "dist",
        "cli",
        "command line",
        "command line interface",
        "http",
        "server",
        "standalone",
        "openapi",
        "yaml",
        "json",
        "toml",
        "javascript",
        "javascript object",
        "typescript",
        "node",
        "execute",
        "script"
    ],
    "scripts": {
        "build": "del ./dist && tsc -p ./tsconfig.json",
        "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec node -r ts-node/register --inspect=0.0.0.0:9229 src/index.ts",
        "lint": "eslint -c .eslintrc.js --ext .ts ./src",
        "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts ./src",
        "start": "node ./dist/index.js"
    },
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/egomobile/swagger-ui-cli.git"
    },
    "bugs": {
        "url": "https://github.com/egomobile/swagger-ui-cli/issues"
    },
    "homepage": "https://github.com/egomobile/swagger-ui-cli#readme",
    "devDependencies": {
        "@egomobile/tsconfig": "^5.0.0",
        "@types/content-type": "1.1.7",
        "@types/js-yaml": "4.0.8",
        "@types/node": "20.8.10",
        "@types/swagger-ui-dist": "3.30.3",
        "del-cli": "5.1.0",
        "eslint": "8.52.0",
        "eslint-config-ego": "^0.17.1",
        "nodemon": "3.0.1",
        "ts-node": "10.9.1",
        "tslint": "6.1.3",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "@egomobile/types": "^2.0.0",
        "@iarna/toml": "2.2.5",
        "axios": "1.6.0",
        "content-type": "1.0.5",
        "is-docker": "3.0.0",
        "js-yaml": "4.1.0",
        "meow": "12.1.1",
        "mrmime": "1.0.1",
        "open": "9.1.0",
        "ora": "7.0.1",
        "sanitize-filename": "1.6.3",
        "swagger-ui-dist": "5.9.1"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
